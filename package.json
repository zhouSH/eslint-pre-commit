{
  "name": "eslint-pre-commit",
  "version": "1.0.6",
  "description": "copy from 3rdeden ,and do some thing  with my feature; for eslint only",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhouSH/eslint-pre-commit.git"
  },
  "scripts": {
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- test.js",
    "example-fail": "echo \"This is the example hook, I exit with 1\" && exit 1",
    "example-pass": "echo \"This is the example hook, I exit with 0\" && exit 0",
    "install": "node install.js",
    "test": "mocha test.js",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- test.js",
    "uninstall": "node uninstall.js",
    "lint": "eslint "
  },
  "keywords": [
    "git",
    "hooks",
    "npm",
    "pre-commit",
    "precommit",
    "run",
    "test",
    "development"
  ],
  "author": "jonex <bozzylee220@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "dependencies": {
    "cross-spawn": "2.0.x",
    "which": "1.2.x",
    "eslint": "^3.0.1"
  },
  "devDependencies": {
    "assume": "1.4.x",
    "istanbul": "0.4.x",
    "mocha": "2.4.x"
  }
}
